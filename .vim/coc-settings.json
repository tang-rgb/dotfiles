{
  "coc.preferences.formatOnSaveFiletypes": [
    "css",
    "markdown",
    "rust"
  ],
  "languageserver": {
    "golang": {
      "command": "gopls",
      "rootPatterns": [
        "go.mod",
        ".vim/",
        ".git/",
        ".hg/"
      ],
      "filetypes": [
        "go"
      ],
      "initializationOptions": {
        "usePlaceholders": true
      }
    }
  }
}
