{
  "go.goplsOptions": {
    "usePlaceholders": false,
    "hoverKind": "SynopsisDocumentation",
    "staticcheck": true,
    "completeUnimported": true,
    "deepCompletion": true,
    "completionDocumentation": true,
    "fuzzyMatching": true,
    "analyses": {
      "unreachable": true,
      "unusedparams": true,
      "fillreturns": true,
      "nonewvars": true,
      "undeclaredname": true
    }
  },
  "coc.preferences.formatOnSaveFiletypes": [
    "css",
    "markdown",
    "go",
    "rust",
    "json"
  ],
  "git.enableGutters": true,
  "git.addGBlameToBufferVar": false
}
