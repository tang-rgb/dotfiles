{
  "diagnostic.level": "hint",
  "languageserver": {
    "golang": {
      "command": "gopls",
      "rootPatterns": [
        "go.mod",
        ".vim/",
        ".git/",
        ".hg/"
      ],
      "filetypes": [
        "go"
      ],
      "initializationOptions": {
        "usePlaceholders": false,
        "hoverKind": "SynopsisDocumentation",
        "staticcheck": true,
        "completeUnimported": true,
        "deepCompletion": true,
        "completionDocumentation": true,
        "fuzzyMatching": true,
        "analyses": {
          "unreachable": true,
          "unusedparams": true,
          "fillreturns": true,
          "nonewvars": true,
          "undeclaredname": true
        }
      }
    }
  },
  "coc.preferences.formatOnSaveFiletypes": [
    "go",
    "css",
    "markdown",
    "rust",
    "json"
  ],
  "list.source.tags.command": "ctags -R -f ./.tags",
  "rust-analyzer.serverPath": "~/.cargo/bin/rust-analyzer",
  "git.enableGutters": true,
  "git.addGBlameToBufferVar": false,
  "cSpell.enabledLanguageIds": [
    "go",
    "javascript",
    "javascriptreact",
    "markdown",
    "plaintext",
    "typescript",
    "typescriptreact",
    "yml"
  ],
  "cSpell.ignorePaths": [
    "node_modules",
    "**/node_modules",
    "**/node_modules/**",
    "node_modules/**",
    "vscode-extension",
    ".git",
    "*.dll",
    "**/*.dll"
  ],
  "cSpell.showStatus": true,
  "cSpell.allowCompoundWords": true,
  "cSpell.ignoreWords": [],
  "explorer.file.showHiddenFiles": true,
  "explorer.keyMappings": {
    "i": false, // cancel default mapkey
    "gk": "expandablePrev",
    "gj": "expandableNext",
    "*": "toggleSelection",
    "<tab>": "actionMenu",
    "h": "collapse",
    "l": [
      "expandable?",
      "expand",
      "open"
    ],
    "J": [
      "toggleSelection",
      "nodeNext"
    ],
    "K": [
      "toggleSelection",
      "nodePrev"
    ],
    "gl": "expandRecursive",
    "gh": "collapseRecursive",
    "<2-LeftMouse>": [
      "expandable?",
      [
        "expanded?",
        "collapse",
        "expand"
      ],
      "open"
    ],
    "o": [
      "expanded?",
      "collapse",
      "expand"
    ],
    "<cr>": [
      "expandable?",
      "expand",
      "open"
    ],
    "e": "open",
    "s": "open:split",
    "S": "open:split:plain",
    "E": "open:vsplit",
    "t": "open:tab",
    "<bs>": "gotoParent",
    "gp": "preview:labeling",
    "y": "copyFilepath",
    "Y": "copyFilename",
    "c": "copyFile",
    "x": "cutFile",
    "p": "pasteFile",
    "d": "delete",
    "D": "deleteForever",
    "a": "addFile",
    "A": "addDirectory",
    "r": "rename",
    ".": "toggleHidden",
    "R": "refresh",
    "?": "help",
    "q": "quit",
    "<esc>": "esc",
    "X": "systemExecute",
    "gd": "listDrive",
    "f": "search",
    "F": "searchRecursive",
    "gf": "gotoSource:file",
    "gb": "gotoSource:buffer",
    "[[": "sourcePrev",
    "]]": "sourceNext",
    "[d": "diagnosticPrev",
    "]d": "diagnosticNext",
    "[c": "gitPrev",
    "]c": "gitNext",
    "<<": "gitStage",
    ">>": "gitUnstage"
  },
  "cSpell.userWords": [
    "Arial",
    "Intellij",
    "Metas",
    "cncb",
    "enduml",
    "icbc",
    "iface",
    "mycnf",
    "mysqld",
    "nvim",
    "plantuml",
    "skinparam",
    "startuml",
    "systemctl",
    "systemd",
    "uagent",
    "ucommon",
    "ucore",
    "udelay",
    "udeploy",
    "uguard",
    "urman"
  ]
}
